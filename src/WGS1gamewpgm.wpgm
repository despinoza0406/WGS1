import nivel.*
import WGS1.*

program SokobanGame {
	game.title("WGS1 : Wollok Gear Solid 1")
	game.title()
	game.height(20)
	game.width(20)
	game.ground("metalwall.png")

	nivel1.cargar()
	
	game.addVisual(snake)
	
	game.whenCollideDo(snake, { something => snake.hittedWith(something) })
	
	game.start()	

}